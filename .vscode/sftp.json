{
    "name": "your-server",
    "protocol": "sftp",
    "uploadOnSave": true,
    "host": "host-from-ssh-config-or-host-address",
    "username": "username",
    "remotePath": "path-at-remote",
    "privateKeyPath": "private-key(rsa/ed25519)",
    "port": 2222,
    "interactiveAuth": true,
    "connectTimeout": 50000,
    "ignoreFile": ".gitignore",
    "ignore": [
        ".vscode",
        ".git",
        ".DS_Store",
        "results",
        "experiments",
        "./data",
        "wandb",
        "docs",
        "build",
        "__pycache__",
        ".idea",
        "artifacts",
        "*.ckpt",
        "*.h5py",
        "*.beton",
        "data_batch_*",
        "*-batches-py",
        "*.out",
        "slurm/",
        "*.gz"
    ]
}
